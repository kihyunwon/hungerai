{
  "name": "Hungerai",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Search for food in image using Clarifai",
  "homepage_url": "http://github.com/kihyunwon/hungerai",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": ["src/clarifai-1.2.0.js", "src/keys.js", "src/background.js", "src/menu.js"],
    "persistent": true
  },
  "options_ui": {
    "page": "src/options.html",
    "chrome_style": true
  }
}
